					<form id="form" action="EmailController" method="post">
						
						<div class="success_wrapper">
							<div class="success-message"><font color="red"><strong>Your Contact form submitted. We'll Contact You Soon.</strong></font></div>
						</div>
						
						<label class="name">
							<input type="text" placeholder="Name:" data-constraints="@Required @JustLetters" name="name" />
							<span class="empty-message">*This field is required.</span>
							<span class="error-message">*This is not a valid name.</span>
						</label>
						<label class="email">
							<input type="text" placeholder="E-mail:" data-constraints="@Required @Email" name="email" />
							<span class="empty-message">*This field is required.</span>
							<span class="error-message">*This is not a valid email.</span>
						</label>
						<label class="phone">
							<input type="text" placeholder="Phone:" data-constraints="@Required @JustNumbers" name="phone"/>
							<span class="empty-message">*This field is required.</span>
							<span class="error-message">*This is not a valid phone.</span>
						</label>
						<label class="message">
							<textarea name="message" placeholder="Message:" data-constraints='@Required @Length(min=20,max=999999)'></textarea>
							<span class="empty-message">*This field is required.</span>
							<span class="error-message">*The message is too short.</span>
						</label>
					
						<div>
